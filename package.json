    "lint": "eslint */**/*.{js,ts,tsx} --quiet --fix",
    "lint:check": "eslint */**/*.{js,ts,tsx}",
    "test": "webpack --config webpack/webpack.dev.js && jest",
    "test:watch": "jest --watch",
    "watch": "webpack --config webpack/webpack.dev.js --watch"
  },
